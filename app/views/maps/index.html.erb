<div id='header'>
	<h1>NYC Real-Time Traffic Cam Map</h1>
</div>

<div id='container'>
	<div id='content'>
		<div class='column' id='right'>
			<h2>Top Links (Highest Average Speed)</h2>
			<ol id='topspeedlist'>
			<% @top_speeds.each do |s| %>
				<li class='topspeeditem'>
					<p><%= "#{s.link.link_name}" %></p>
					<p><%= "#{s.speed} mph" %></p>
				</li>
			<% end %>
			</ol>
		</div>

		<div id='center'>

		<%= gmaps( {
				"polylines" => { "data" => @data }
		}) %>

		</div>


	</div>
</div>


<div id='footer'>
	<p>Map created by Yee-Ning Cheng</p>
	<p>Data provided by: NYCDOT</p>
</div>

<script type='text/javascript'>
$(document).ready(function() {
	$("li.topspeeditem").click(function() {
		alert('what up');
	})
})
</script>